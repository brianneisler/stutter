  "dependencies": {
    "buffer": "^5.2.1",
    "core-js": "^3.3.6",
    "graphlib": "^2.1.7",
    "immutable": "^4.0.0-rc.12",
    "is-url": "^1.2.4",
    "mime-types": "^2.1.22",
    "source-map-support": "^0.5.5",
    "string-to-stream": "^1.1.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "babel-eslint": "^10.0.3",
