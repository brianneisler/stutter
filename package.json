  "dependencies": {
    "buffer": "^5.6.0",
    "core-js": "^3.6.5",
    "graphlib": "^2.1.8",
    "immutable": "^4.0.0-rc.12",
    "is-url": "^1.2.4",
    "mime-types": "^2.1.27",
    "source-map-support": "^0.5.19",
    "string-to-stream": "^3.0.1",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-eslint": "^10.1.0",
