#!/usr/bin/env node

var program = require('commander')

program
  .description('parse a stutter js file')
  .usage('[options] <file>')
  .arguments('<file>')
  .action(function(file, options) {
    var parse = require('../').commands.parse
    parse(file, options)
      .then(function() {
        process.exit()
      })
      .catch(function() {
        process.exit(1)
      })
  })
  .parse(process.argv)
