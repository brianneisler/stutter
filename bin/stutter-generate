#!/usr/bin/env node

var program = require('commander')

program
  .description('generate code from an ast')
  .usage('[options] <file>')
  .arguments('<file>')
  .action(function(file, options) {
    var evaluate = require('../').commands.evaluate
    evaluate(file, options)
      .then(function() {
        process.exit()
      })
      .catch(function() {
        process.exit(1)
      })
  })
  .parse(process.argv)
